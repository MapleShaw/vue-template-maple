{
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "nodeVersion": "22.11.0",
 
  "redirects": [
    {
      "source": "/price",
      "destination": "/product",
      "statusCode": 302
    }
  ],
  "rewrites": [
    {
      "source": "/static-page.html",
      "destination": "/static-test.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}